<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="600.0" prefWidth="100.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.apekshapms.controller.sidebar.LabAssistantSideBarController">
   <children>
      <VBox prefHeight="600.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Button fx:id="addReportButton" mnemonicParsing="false" onAction="#handleAddReportButtonAction" prefHeight="25.0" prefWidth="101.0" text="Add Report" />
            <Button fx:id="addDetailsButton" mnemonicParsing="false" onAction="#handleAddDetailsButtonAction" prefHeight="25.0" prefWidth="102.0" text="Add Details" />
            <Button fx:id="checkReportButton" mnemonicParsing="false" onAction="#handleCheckReportButtonAction" prefHeight="25.0" prefWidth="101.0" text="Check Report" />
         </children>
      </VBox>
   </children>
</AnchorPane>
