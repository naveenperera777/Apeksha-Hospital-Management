   <children>
      <VBox prefHeight="600.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Button fx:id="newPatientButton" mnemonicParsing="false" onAction="#handleNewPatientButtonAction" prefHeight="25.0" prefWidth="101.0" text="New Patient" />
            <Button fx:id="searchPatientButton" mnemonicParsing="false" onAction="#handleViewPatientAction" prefHeight="25.0" prefWidth="102.0" text="View Patient" />
         </children>
      </VBox>
   </children>
</AnchorPane>
